			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#636e85"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"entity.name.variable.parameter"
			],
			"settings": {
				"foreground": "#e1dfdd"
			}
		},
		{
			"name": "Object properties",
			"scope": [
				"meta.object-literal",
				"meta.object.member",
				"meta.objectliteral"
			],
			"settings": {
				"foreground": "#d8dedc"
			}
		},
		{
			"name": "TS/JS module",
			"scope": ["support.type.object.module"],
			"settings": {
				"foreground": "#8FBCBB"
			}
		},
		{
			"scope": "punctuation",
			"settings": {
				"foreground": "#e1dfdd"
			}
		},
		{
